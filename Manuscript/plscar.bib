	journal = {Proceedings of the National Academy of Sciences},
	author = {Kauwe, John S. K. and Cruchaga, Carlos and Mayo, Kevin and Fenoglio, Chiara and Bertelsen, Sarah and Nowotny, Petra and Galimberti, Daniela and Scarpini, Elio and Morris, John C. and Fagan, Anne M. and Holtzman, David M. and Goate, Alison M.},
